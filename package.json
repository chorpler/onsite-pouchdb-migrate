{
  "name": "pouchdb-migrate",
  "version": "7.5.0",
  "description": "PouchDB plugin for running migrations, forked for PouchDB 7.0.0+",
  "author": "David Sargeant <chorpler@gmail.com>",
  "homepage": "https://github.com/chorpler/onsite-pouchdb-migrate#readme",
  "contributors": [
    {
      "name": "Johannes J. Schmidt",
      "email": "schmidt@netzmerk.com",
      "url": "http://die-tf.de/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/chorpler/onsite-pouchdb-migrate"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "upsert",
    "putIfNotExists",
    "couch",
    "couchdb"
  ],
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "git://github.com/chorpler/onsite-pouchdb-migrate"
  },
  "files": [
    "compiled",
    "dist",
    "docs",
    "lib",
    "src",
    "types"
  ],
  "main": "lib/index.js",
  "jsnext:main": "lib/index.es.js",
  "module": "lib/index.es.js",
  "browser": "lib/index.browser.js",
  "types": "types/index.d.ts",
  "brVars": {
    "name": "PouchMigratePlugin",
    "package": "pouchdb.migrate.plugin"
  },
  "scripts": {
    "clrscr": "node bin/clear_terminal.js",
    "clrscroll": "node bin/itermclear.js",
    "clean": "rimraf lib/* dist/* compiled/* types/* && mkdirp lib dist compiled types",
    "compile": "tsc -p ./tsconfig.json",
    "rollup-cjs": "rollup -c config/rollup.config.cjs.js && rollup -c config/rollup.config.browser.cjs.js",
    "rollup-es": "rollup -c config/rollup.config.es.js && rollup -c config/rollup.config.browser.es.js",
    "rollup": "npm-run-all --parallel rollup-cjs rollup-es",
    "browserify:rollup": "browserify -t [ babelify --global --presets [ @babel/preset-env ] ] -t brfs -p bundle-collapser/plugin -s $npm_package_brVars_name lib/index.browser.js > dist/$npm_package_brVars_package.js",
    "browserify": "npm run browserify:rollup",
    "minify": "uglifyjs -mc < dist/$npm_package_brVars_package.js > dist/$npm_package_brVars_package.min.js",
    "build:prod": "npm-run-all clean compile rollup browserify minify",
    "build:dev": "npm-run-all clean compile rollup browserify",
    "build": "npm run build:prod",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/",
    "lintOld": "eslint bin/ src/ test/",
    "dev": "npm run test-local",
    "test": "tape test/test.js | tap-spec",
    "testOld": "npm run clrscroll && npm run build && node bin/run-test.js",
    "test-local": "cross-env CLIENT=local npm run test",
    "test-node": "cross-env CLIENT=node TEST_DB=testdb,http://localhost:5984/testdb npm run test",
    "test-browser": "cross-env CLIENT=ChromeHeadless npm run test",
    "test-types": "tsc --noEmit -p types",
    "release": "standard-version"
  },
  "dependencies": {
    "async": "1.5.2",
    "crypto-browserify": "^3.12.0",
    "inherits": "2.0.4",
    "pouchdb-binary-utils": "../lib/pouchdb-binary-utils-7.5.0.tgz",
    "pouchdb-core": "../lib/pouchdb-core-7.5.0.tgz",
    "pouchdb-fetch": "../lib/pouchdb-fetch-7.5.0.tgz",
    "pouchdb-md5": "file:../lib/pouchdb-md5-7.5.0.tgz",
    "pouchdb-utils": "../lib/pouchdb-utils-7.5.0.tgz",
    "spark-md5": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@nut-tree/nut-js": "^2.2.0",
    "@onsite/pouchdb-debug": "file:../lib/onsite-pouchdb-debug-7.5.0.tgz",
    "@onsite/pouchdb-memory": "^7.0.0-rc.1",
    "@types/debug": "^4.1.1",
    "@types/node": "^12.20.55",
    "@types/node-fetch": "^2.1.6",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-core": "^7.0.10",
    "@types/pouchdb-find": "^6.3.7",
    "@types/pouchdb-replication": "^6.4.4",
    "@types/spark-md5": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "add-cors-to-couchdb": "0.0.6",
    "babelify": "^10.0.0",
    "brfs": "^2.0.1",
    "browserify": "^16.2.3",
    "bundle-collapser": "^1.3.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.2.0",
    "es3ify": "^0.2.2",
    "es6-promise": "^4.2.6",
    "eslint": "^8.23.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.10.1",
    "json8": "^0.9.2",
    "karma": "^4.0.0",
    "karma-browserify": "^6.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^2.0.2",
    "memdown": "6.1.1",
    "mkdirp": "^0.5.1",
    "mocha": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "pouchdb-adapter-http": "file:../lib/pouchdb-adapter-http-7.5.0.tgz",
    "pouchdb-adapter-indexeddb": "file:../lib/pouchdb-adapter-indexeddb-7.5.0.tgz",
    "pouchdb-adapter-leveldb": "file:../lib/pouchdb-adapter-leveldb-7.5.0.tgz",
    "pouchdb-find": "file:../lib/pouchdb-find-7.5.0.tgz",
    "pouchdb-mapreduce": "file:../lib/pouchdb-mapreduce-7.5.0.tgz",
    "pouchdb-replication": "file:../lib/pouchdb-replication-7.5.0.tgz",
    "promise-polyfill": "^8.2.0",
    "puppeteer": "^17.1.3",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-inject": "^2.2.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-typescript2": "^0.19.2",
    "standard-version": "^5.0.0",
    "tap-spec": "5.0.0",
    "tape": "5.6.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2",
    "uglify-js": "^3.4.9",
    "watchify": "^4.0.0",
    "whatwg-fetch": "^3.0.0",
    "zuul": "^3.12.0"
  },
  "scriptsComments": {
    "build": "mkdir -p dist && browserify index.js -o dist/pouchdb.migrate.js && npm run min",
    "min": "uglifyjs dist/pouchdb.migrate.js -mc > dist/pouchdb.migrate.min.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "tape test.js | tap-spec"
  },
  "dependenciesComments": {
    "async": "^1.5.1",
    "pouchdb": "^5.0.0",
    "pouchdb-extend": "^0.1.2",
    "spark-md5": "^2.0.0"
  },
  "devDependenciesComments": {
    "browserify": "^13.0.0",
    "memdown": "^1.1.0",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.1",
    "uglify-js": "^2.5.0",
    "semantic-release": "^6.0.3"
  },
  "browserComments": {
    "crypto": false
  }
}
